{
  "version": 2,
  "name": "medical-triage-api",
  "builds": [
    {
      "src": "apps/api/src/main.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["apps/api/prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api/src/main.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "installCommand": "pnpm install",
  "buildCommand": "pnpm --filter api build",
  "outputDirectory": "apps/api/dist"
}
